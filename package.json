{
  "name": "matchbox-ui",
  "version": "0.9.0",
  "description": "Matchbox ui toolkit",
  "main": "index.js",
  "scripts": {
    "test": "ELECTRON_PATH=./node_modules/.bin/electron ./node_modules/.bin/electron-mocha --renderer --recursive --bail --ui bdd ./test/",
    "build-standalone": "browserify index.js --standalone matchboxUi -o build/matchbox-ui.js",
    "build-debug": "browserify index.js --standalone matchboxUi --debug -o build/matchbox-ui.debug.js",
    "build": "npm run build-standalone && npm run build-debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matchboxjs/matchbox-ui.git"
  },
  "keywords": [
    "matchbox",
    "ui",
    "view"
  ],
  "author": "tunderdomb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matchboxjs/matchbox-ui/issues"
  },
  "homepage": "https://github.com/matchboxjs/matchbox-ui#readme",
  "dependencies": {
    "backyard": "^0.1.0",
    "offspring": "^0.4.0",
    "stations": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^3.4.0",
    "electron-mocha": "^0.8.0",
    "electron-prebuilt": "^0.36.5"
  }
}
