{
  "name": "matchbox-ui",
  "version": "0.7.0",
  "description": "Matchbox ui toolkit",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha",
    "build-standalone": "browserify index.js --standalone matchboxUi -o matchbox-ui.js",
    "build-debug": "browserify index.js --standalone matchboxUi --debug -o matchbox-ui.debug.js",
    "build": "npm run build-standalone && npm run build-debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matchboxjs/matchbox-ui.git"
  },
  "keywords": [
    "matchbox",
    "ui",
    "view"
  ],
  "author": "tunderdomb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matchboxjs/matchbox-ui/issues"
  },
  "homepage": "https://github.com/matchboxjs/matchbox-ui#readme",
  "dependencies": {
    "matchbox-dom": "^0.6.4",
    "matchbox-factory": "^0.2.0",
    "matchbox-radio": "^1.0.0",
    "matchbox-util": "^0.4.1"
  },
  "devDependencies": {
    "browserify": "^12.0.0",
    "chai": "^3.4.0",
    "mocha": "^2.3.3"
  }
}
